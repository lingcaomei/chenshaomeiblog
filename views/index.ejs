<% include include/header%>
<% include include/nav%>

<div class="container">
    <!-- articles是render传出来的对象的值即--》articles--》 {title:'xxx',articles:[]}-->
    <% if (articles.length) { %>

        <% articles.forEach(function(article){ %>

        <div class="media">
            <div class="media-left">
                <a href="#">
                    <img data-holder-rendered="true" src="data:image/svg+xml;base64," style="width: 64px; height: 64px;" class="media-object" data-src="holder.js/64x64" alt="64x64">
                </a>
            </div>
            <div class="media-body">
                <h4 class="media-heading"><%= article.title %></h4>
                <%= article.content %>
            </div>
        </div>
        <% }) %>

    <% } %>


</div>


<% include include/footer%>